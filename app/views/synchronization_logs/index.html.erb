<% provide :navigation_synchronization_logs, "active" %>
<h2>All synchronization logs</h2>
<hr/>
<table class="table table-hover talbe-bordered">
  <thead>
    <tr>
      <th>Index</th>
      <th>Status</th>
      <th>Content</th>
      <th>Created at</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "synchronization_log", collection: @synchronization_logs, as: :synchronization_log %>
  </tbody>
</table>
<%= paginate @synchronization_logs %>
